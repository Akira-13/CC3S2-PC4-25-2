# Puerto interno donde corre la app web
APP_PORT=8000

# Nivel de logs de la app (INFO, DEBUG, WARNING)
LOG_LEVEL=INFO

# Carpeta donde la app escribe logs crudos dentro del contenedor
RAW_LOG_PATH=/var/log/app/raw

# Carpeta donde el procesador escribe los logs anonimizados
SANITIZED_LOG_PATH=/var/log/app/sanitized

# Carpeta donde se guardar√°n los backups cifrados
BACKUP_OUTPUT_PATH=/backups

# Tipo de cifrado para el servicio de backup 
BACKUP_ENCRYPTION=simulated
# Opcional: clave para cifrado 
BACKUP_KEY=example-key-goes-here
